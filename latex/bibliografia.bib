@inproceedings{santos_um_2019,
	title = {Um Modelo para Detecção de Ansiedade Computacional em Idosos Interagindo com a Web},
	rights = {Copyright (c)},
	url = {https://sol.sbc.org.br/index.php/ihc_estendido/article/view/8430},
	doi = {10.5753/ihc.2019.8430},
	abstract = {With the increasing use of different types of technologies in everyday life, one would expect that the use of computers would be comfortable for everyone. However, some people still feel apprehensive before and during computer use, experiencing multiple levels of anxiety and, for high levels, exhibit what is known as Computer Anxiety ({CA}). People with {CA} face problems using technologies in different contexts, including situations in which they use computers because they have no alternative, such as using government services available only through computer systems, exercising citizenship by voting with electronic voting machines, {ATMs} and using computers at work. Computer Anxiety can create multiple barriers for these people, depriving them of access to technology, education, full exercise of citizenship, among others. In this context, the present project investigated the main characteristics related to how elderly people with Computer Anxiety interact with computers and how these characteristics are represented in interaction logs. To this end, a field study was conducted with 31 elderly participants involving the capture of data related to interaction, eye tracking data, depression and anxiety levels screening scales, computational anxiety screening scales, among others. Based on the results, a model for Computer Anxiety detection was developed. This model is able to identify different levels of Computer Anxiety (i.e., no anxiety, low level of Computer Anxiety and high level of Computer Anxiety) with accuracy of 83.87\%, showing that it is possible to identify different levels of Computer Anxiety of elderly people interacting with the Web through analysis of interaction data logs.},
	eventtitle = {Anais Estendidos do {XVIII} Simpósio Brasileiro sobre Fatores Humanos em Sistemas Computacionais},
	pages = {182--183},
	booktitle = {Anais Estendidos do Simpósio Brasileiro de Fatores Humanos em Sistemas Computacionais ({IHC})},
	publisher = {{SBC}},
	author = {Santos, Thiago dos and Santana, Vagner},
	urldate = {2022-11-23},
	date = {2019-10-11},
	langid = {portuguese},
	note = {{ISSN}: 0000-0000},
