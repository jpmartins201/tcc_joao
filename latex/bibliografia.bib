@article{othman_usability_2022,
	title = {Usability evaluation of a virtual reality smartphone app for a living museum},
	volume = {21},
	issn = {1615-5289, 1615-5297},
	url = {https://link.springer.com/10.1007/s10209-021-00820-4},
	doi = {10.1007/s10209-021-00820-4},
	abstract = {This paper elaborates the empirical evidence of a usability evaluation of a {VR} and non-{VR} virtual tour application for a living museum. The System Usability Scale ({SUS}) was used in between participants experiments (Group 1: non-{VR} version and Group 2: {VR} version) with 40 participants. The results show that the mean scores of all components for the {VR} version are higher compared to the non-{VR} version, overall {SUS} score (72.10 vs 68.10), usability score (75.50 vs 71.70), and learnability (58.40 vs 57.00). Further analysis using a two-tailed independent t test showed no difference between the non-{VR} and {VR} versions. Additionally, no significant difference was observed between the groups in the context of gender, nationality, and prior experience (other {VR} tour applications) for overall {SUS} score, usability score, and learnability score. Α two-tailed independent t test indicated no significant difference in the usability score between participants with {VR} experience and no {VR} experience. However, a significant difference was found between participants with {VR} experience and no {VR} experience for both {SUS} score (t(38) = 2.17, p = 0.037) and learnability score (t(38) = 2.40, p = 0.021). The independent t test results indicated a significant difference between participant with and without previous visits to {SCV} for the usability score (t(38) = −2.31, p = 0.027), while there was no significant differences observed in other components. It can be concluded that both versions passed based on the {SUS} score. However, the sub-scale usability and learnability scores indicated some usability issue.},
	pages = {995--1012},
	number = {4},
	journaltitle = {Universal Access in the Information Society},
	shortjournal = {Univ Access Inf Soc},
	author = {Othman, Mohd Kamal and Nogoibaeva, Altynai and Leong, Lai San and Barawi, Mohamad Hardyman},
	urldate = {2022-12-12},
	date = {2022-11},
	langid = {english},
	file = {Othman et al. - 2022 - Usability evaluation of a virtual reality smartpho.pdf:C\:\\Users\\Websis\\Zotero\\storage\\SCN5T6YE\\Othman et al. - 2022 - Usability evaluation of a virtual reality smartpho.pdf:application/pdf},
}

@inproceedings{jin_i_2022,
	location = {Athens Greece},
	title = {“I Used To Carry A Wallet, Now I Just Need To Carry My Phone”: Understanding Current Banking Practices and Challenges Among Older Adults in China},
	isbn = {978-1-4503-9258-7},
	url = {https://dl.acm.org/doi/10.1145/3517428.3544820},
	doi = {10.1145/3517428.3544820},
	shorttitle = {“I Used To Carry A Wallet, Now I Just Need To Carry My Phone”},
	abstract = {Managing fnances is crucial for older adults who are retired and may rely on savings to ensure their lives’ quality. As digital banking platforms (e.g., mobile apps, electronic payment) gradually replace physical ones, it is critical to understand how they adapt to digital banking and the potential frictions they experience. We conducted semi-structured interviews with 16 older adults in China, where the aging population is the largest and digital banking grows fast. We also interviewed bank employees to gain complementary perspectives of these help givers. Our fndings show that older adults used both physical and digital platforms as an ecosystem based on perceived pros and cons. Perceived usefulness, self-confdence, and social infuence were key motivators for learning digital banking. They experienced app-related (e.g., insufcient error-recovery support) and user-related challenges (e.g., trust, security and privacy concerns, low perceived self-efcacy) and developed coping strategies. We discuss design considerations to improve their banking experiences.},
