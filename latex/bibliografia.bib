@inproceedings{poositaporn_design_2022,
	title = {Design of Weighted Usability Model for Elderly/Young and General/Military User Groups},
	doi = {10.1109/BigComp54360.2022.00067},
	abstract = {Information services should support good usability to satisfy users' needs. However, measuring the usability has difficulty due to a lack of practical approaches as well as the consideration of many factors such as aging, computer experience, and physical abilities. This study introduces a weighted usability model based on the Analytic Hierarchy Process({AHP}), a multi-criteria decision making by calculating scale rating ratio and producing a set of weights. We experiment with the proposed model to evaluate three user groups: elderly, young, and military groups with six information services: {ScienceON}, {RISS}, Web of Science, {IEEE} Xplore, {DTIMS}, and Air university library. The experimental results show that our approach works well in prioritizing attractiveness and learnability for elderly group, searchability for young group, and understandability and searchability for military user group.},
	eventtitle = {2022 {IEEE} International Conference on Big Data and Smart Computing ({BigComp})},
	pages = {310--313},
	booktitle = {2022 {IEEE} International Conference on Big Data and Smart Computing ({BigComp})},
	author = {Poositaporn, Athiruj and Onuean, Athita and Jung, Hanmin},
	date = {2022-01},
	note = {{ISSN}: 2375-9356},
	keywords = {Analytic Hierarchy Process, Analytical models, Atmospheric modeling, Computational modeling, Data models, Libraries, Multi-Criteria Decision Making, Navigation, Usability Evaluation, User interfaces, Weighted Usability Model},
}
